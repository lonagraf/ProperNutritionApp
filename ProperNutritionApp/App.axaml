<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
             xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             x:Class="ProperNutritionApp.App"
             RequestedThemeVariant="Default">
    <Application.Resources>
        <themes:BundledTheme BaseTheme="Light" PrimaryColor="LightBlue" SecondaryColor="Purple"/>
        
    </Application.Resources>
    <Application.Styles>
        <StyleInclude Source="avares://OxyPlot.Avalonia/Themes/Default.axaml"/>
        <themes:MaterialTheme PrimaryColor="Purple" SecondaryColor="Lime" />
        <avalonia:MaterialIconStyles />
    </Application.Styles>
    
</Application>